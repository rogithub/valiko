{
  "name": "valiko",
  "version": "1.0.0",
  "description": "validation utils for knockout",
  "browser": "valiko-min.js",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "build": "rm -rf ./lib/ && tsc --declaration",
    "test": "nyc mocha ./test/*.ts && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogithub/valiko.git"
  },
  "keywords": [
    "knockout",
    "validation"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Rodrigo Jimenez <correo.rodrigo@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rogithub/valiko/issues"
  },
  "homepage": "https://github.com/rogithub/valiko#readme",
  "devDependencies": {
    "@types/knockout": "^3.4.66",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "coveralls": "^3.0.6",
    "knockout": "^3.5.0",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "knockout": "^3.5.0"
  }
}