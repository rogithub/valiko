{
  "name": "valiko",
  "version": "1.0.0",
  "description": "validation utils for knockout",
  "browser": "valiko-min.js",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "build": "tsc",
    "start": "npm run build",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogithub/valiko.git"
  },
  "keywords": [
    "knockout",
    "validation"
  ],
  "author": "Rodrigo Jimenez <correo.rodrigo@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rogithub/valiko/issues"
  },
  "homepage": "https://github.com/rogithub/valiko#readme",
  "devDependencies": {
    "@types/knockout": "^3.4.66",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.3",
    "knockout": "^3.4.66"
  },
  "peerDependencies": {
    "knockout": "^3.4.66"
  }
}
